<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.newMemberSignUpController">
   <children>
      <AnchorPane prefHeight="100.0" prefWidth="800.0" style="-fx-background-color: black;">
         <children>
            <Label layoutX="41.0" layoutY="31.0" text="Build Gym - Member Signup" textFill="WHITE">
               <font>
                  <Font name="Century Gothic" size="30.0" />
               </font>
            </Label>
         </children></AnchorPane>
      <AnchorPane layoutY="100.0" prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: #2C394B;">
         <children>
            <Button fx:id="confirmMemberSignUp" layoutX="93.0" layoutY="363.0" mnemonicParsing="false" onAction="#registerMemberButtonOnAction" prefHeight="25.0" prefWidth="63.0" text="Confirm" />
            <Label layoutX="93.0" layoutY="80.0" text="Name" textFill="WHITE" />
            <TextField fx:id="getMemberName" layoutX="93.0" layoutY="98.0" prefHeight="25.0" prefWidth="604.0" promptText="Name">
               <effect>
                  <DropShadow height="56.63" radius="27.815" width="56.63" />
               </effect>
            </TextField>
            <Label layoutX="93.0" layoutY="135.0" prefHeight="17.0" prefWidth="52.0" text="Gender" textFill="WHITE" />
            <Label layoutX="93.0" layoutY="197.0" prefHeight="17.0" prefWidth="74.0" text="Weight(kg)" textFill="WHITE" />
            <Label layoutX="93.0" layoutY="250.0" prefHeight="17.0" prefWidth="63.0" text="Height(cm)" textFill="WHITE" />
            <TextField fx:id="getMemberGender" layoutX="93.0" layoutY="153.0" prefHeight="25.0" prefWidth="604.0" promptText="Gender">
               <effect>
                  <DropShadow height="56.63" radius="27.815" width="56.63" />
               </effect>
            </TextField>
            <TextField fx:id="getMemberWeight" layoutX="93.0" layoutY="215.0" prefHeight="25.0" prefWidth="604.0" promptText="Weight">
               <effect>
                  <DropShadow height="56.63" radius="27.815" width="56.63" />
               </effect>
            </TextField>
            <TextField fx:id="getMemberHeight" layoutX="93.0" layoutY="272.0" prefHeight="25.0" prefWidth="604.0" promptText="Height">
               <effect>
                  <DropShadow height="56.63" radius="27.815" width="56.63" />
               </effect>
            </TextField>
            <Button fx:id="cancelMemberSignUpButton" layoutX="156.0" layoutY="363.0" mnemonicParsing="false" onAction="#cancelMemberSignUpButtonOnAction" prefHeight="25.0" prefWidth="63.0" text="Back" />
            <Label fx:id="CreateMemberMessageLabel" layoutX="95.0" layoutY="331.0" prefHeight="17.0" prefWidth="248.0" textFill="WHITE">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
